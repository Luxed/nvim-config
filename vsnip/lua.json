{
    "If": {
        "prefix": ["if"],
        "body": [
            "if $1 then",
            "\t$0",
            "end"
        ],
        "description": "lua if"
    },
    "Function": {
      "prefix": ["fn"],
      "body": [
        "function $1($2)",
        "\t$0",
        "end"
      ]
    },
    "Local Function": {
      "prefix": ["lfn"],
      "body": [
        "local function $1($2)",
        "\t$0",
        "end"
      ]
    }
}
